@{
    string judul = "ASP.NET Core Training";
}

<html>
    <head>
    <title>Home Page</title>
    </head>
    <body>
        <h1>Welcome to ASP.NET Core Training</h1>
        <p>Username: @ViewBag.username</p>
        <p>Judul Training: @judul</p>
        <p>
            <ul>
            @foreach (string nama in ViewBag.users)
            {
                <li>@nama</li>
            }
            </ul>
        </p>
    </body>
</html>
